<h1>Profiles#new</h1>
<p>Find me in app/views/profiles/new.html.erb</p>

<%= form_for @profile do |d| %>
  <p class="postForm">
    <%= d.label :username %><br><br>
    <%= d.text_field :username %>
  </p>
 
  <p class="postForm">
    <%= d.label :first_name %><br><br>
    <%= d.text_field :first_name %>
  </p>

  <p class="postForm">
    <%= d.label :last_name %><br><br>
    <%= d.text_field :last_name %>
  </p>
 
 <p class="postForm">
    <%= d.label :country %><br><br>
    <%= d.text_field :country %>
  </p>

  <p class="postForm">
    <%= d.label :avatar %><br><br>
    <%= d.text_field :avatar %>
  </p>
  <p class="postForm">
    <%= d.submit %>
  </p>
<% end %>